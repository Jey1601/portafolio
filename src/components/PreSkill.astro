--- 
import '../styles/global.css'

import type {PreSkill} from '../interfaces/interface';
import { Image } from 'astro:assets';

const {src_image, name_skill} = Astro.props as PreSkill;
--- 

<div class="popUp">
    <a href="./skills">
    <Image
        src={src_image}
        alt={name_skill}
        width={100}
        height={100}
        style="width: 2.5vw;"
        />
    </a>    
</div>