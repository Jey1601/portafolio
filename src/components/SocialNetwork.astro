--- 
import '../styles/global.css'

import type { SocialNetwork } from "../interfaces/interface";
const { src_image, name_network, url } = Astro.props as SocialNetwork;

import { Image } from "astro:assets";
--- 

<div class="popUp">
    <a href={url} target="_blank">
        <Image
            src={src_image}
            alt={name_network}
            width={100}
            height={100}
            style="width: 1.5vw;"
        />
    </a>
</div>